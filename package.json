{
  "name": "basecred",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "dev": "pnpm --filter interface dev",
    "start": "pnpm --filter docs start",
    "dev:all": "concurrently -n \"INTERFACE,DOCS\" -c \"cyan,magenta\" \"pnpm dev\" \"pnpm start\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}